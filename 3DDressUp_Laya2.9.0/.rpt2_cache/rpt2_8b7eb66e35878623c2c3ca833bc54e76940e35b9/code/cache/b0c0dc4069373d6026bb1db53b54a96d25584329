        this.promoItem.onClick_ = () => { this.LoadAndShowIcon(); };\r\n            this.promoItem.DoLoad();\r\n        }\r\n        return data;\r\n    }\r\n    LoadAndShowIcon() {\r\n        if (this.LoadIcon() != null) {\r\n            this.promoItem.OnShow();\r\n        }\r\n        else {\r\n            if (this.promoItem.data == null) {\r\n                this.owner.destroy();\r\n            }\r\n        }\r\n    }\r\n}\r\nP201.style = \"P201\";\r\nP201.promoList = null;\r\n",
